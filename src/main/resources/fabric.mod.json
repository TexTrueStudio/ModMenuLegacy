{
  "schemaVersion": 1,
  "id": "modmenu",
  "name": "Mod Menu",
  "version": "${version}",
  "environment": "client",
  "license": "MIT",
  "icon": "assets/modmenu/icons/icon.png",
  "entrypoints": {
    "client": [
      "com.terraformersmc.modmenu.ModMenu"
    ],
    "modmenu": [
      "com.terraformersmc.modmenu.ModMenuModMenuCompat"
    ]
  },
  "contact": {
    "homepage": "https://minecraft.curseforge.com/projects/modmenu",
    "issues": "https://github.com/Prospector/ModMenu/issues"
  },
  "authors": [
    "TexTrue, TexTrueStudio", "Prospector", "ENDERZOMBI102"
  ],
  "description": "Adds a mod menu to view the list of mods you have installed.",
  "mixins": [
    "mixins.modmenu.json"
  ],
  "depends": {
    "fabric-api": "*",
    "fabricloader": ">=0.11.1",
    "minecraft": "1.10.x"
  }
}
